{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "html",
      "vue"
    ],
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "css.validate": false,
    "eslint.codeAction.showDocumentation": {
      "enable": true
    },
    "editor.rulers": [
      120
    ],
    "editor.selectionHighlight": false,
    "stylelint.validate": [
      "css",
      "less",
      "postcss",
      "vue",
      "scss",
      "sass"
    ],
    "preCI.localCodeCheck.filter.whitePath": "",
    "preCI.localCodeCheck.filter.skipPath": ".*/\\.codecc/.*",
    "preCI.localCodeCheck.preCommit": "none",
    "preCI.localCodeCheck.prePush": "none",
    "preCI.pipeline.yamlFilePath": "build.yml",
    "preCI.localCodeCheck.toolType": "binary",
    "preCI.localCodeCheck.checkerSet.Other": [
      "standard_scc"
    ],
    "preCI.localCodeCheck.checkerSet.JS": [
      "standard_js"
    ],
    "preCI.localCodeCheck.checkerSet.TS": [
      "ts_standard"
    ]
  }
